@model Shared.Models.ProposalModel
<tr>
    <td>@Model.Title</td>
    <td>@Model.Speaker</td>
    <td>
        @if (Model.Approved)
        {
            <div class="fa fa-check"></div>
        }
    </td>
    <td>
        @if (!Model.Approved)
        {
            <a asp-controller="Proposal" asp-action="Approve" asp-route-proposalId="@Model.Id">Approve</a>
        }
    </td>
</tr>